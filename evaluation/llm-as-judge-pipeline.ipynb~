    "## IMPORTS\n",
    "from concurrent.futures import ThreadPoolExecutor, as_completed\n",
    "from tqdm import tqdm\n",
    "import cohere\n",
    "import json\n",
    "import os\n",
    "import re\n",
    "import csv\n",
    "\n"
   ],
   "outputs": [],
   "execution_count": null
